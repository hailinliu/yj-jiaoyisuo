<?xml version="1.0" encoding="utf-8" standalone="no"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#00ffffff"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:padding="5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/lib_shape_version_white"
        android:orientation="vertical"
        android:paddingStart="50pt"
        android:paddingTop="30pt"
        android:paddingEnd="50pt"
        android:paddingBottom="30pt">

        <TextView
            android:id="@+id/tvTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15pt"
            android:gravity="center_horizontal"
            android:text="@string/libappCircleCheckUtil1"
            android:textColor="@color/libTextWhite"
            android:textSize="32pt"
            android:textStyle="bold" />
        <ImageView
            android:layout_width="200pt"
            android:layout_marginTop="40pt"
            android:src="@mipmap/lib_cancel"
            android:layout_gravity="center"
            android:layout_height="200pt"/>
        <TextView
            android:id="@+id/tvContent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="50pt"
            android:layout_marginBottom="30pt"
            android:textColor="@color/libTextHint"
            android:textSize="28pt"
            tools:text="---" />

        <include layout="@layout/lib_include_cancel_sure" />
    </LinearLayout>
</LinearLayout>
