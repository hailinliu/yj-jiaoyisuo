<?xml version="1.0" encoding="utf-8" standalone="no"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--this drawable is for thumb when pressing-->
    <item
        android:state_selected="true">
        <shape >
            <solid android:color="@color/bibiGreen" />
            <size
                android:width="9pt"
                android:height="18pt" />
        </shape>
    </item>
    <!--for thumb in normal-->
    <item >
        <shape >
            <solid android:color="@color/bibiTextHint" />
            <size
                android:width="9pt"
                android:height="18pt" />
        </shape>
    </item>
</selector>
