<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.shizhefei.view.coolrefreshview.CoolRefreshView
        android:id="@+id/coolRefreshView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:nestedScrollingEnabled="false" />
    </com.shizhefei.view.coolrefreshview.CoolRefreshView>
    <TextView
        android:id="@+id/level_all_pc"
        android:layout_width="110pt"
        android:layout_height="110pt"
        android:layout_marginEnd="20pt"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="80pt"
        android:gravity="center"
        android:background="@drawable/level_oval_shape"
        android:padding="20pt"
        android:text="@string/levellevel_activity_trade270"
        android:textColor="@color/levelBg"
        android:textSize="27pt" />
</RelativeLayout>
