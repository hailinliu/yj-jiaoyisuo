<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<resources xmlns:tools="http://schemas.android.com/tools">


    <style name="levelDialogStyle" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>              <!-- 设置title -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <!-- 设置边框 -->
        <item name="android:windowIsTranslucent">true</item>        <!-- 设置半透明 -->
        <!--<item name="android:activityOpenEnterAnimation">@anim/fabi_slide_left_in</item>-->
        <!--<item name="android:activityCloseExitAnimation">@anim/fabi_slide_left_out</item>-->
        <!--可以设置Activity的弹出和消失动画（可选）-->
        <item name="android:windowAnimationStyle">@style/levelSlideAnim</item>

    </style>
    <style name="levelSlideAnim">
        <item name="android:windowEnterAnimation">@anim/level_slide_left_in</item>
        <item name="android:windowExitAnimation">@anim/level_slide_left_out</item>
    </style>

    <style name="levelButtonSure" parent="libButtonStyle.Sure">

    </style>
    <style name="levelButtonCancel" parent="libButtonStyle.Cancel">

    </style>

</resources>
